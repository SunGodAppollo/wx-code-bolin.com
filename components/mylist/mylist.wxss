/* components/mylist/mylist.wxss */
.mylist {
	background-color: #fff;
}
.mylist:active {
	opacity: 0.8;
}
.mylist-box {
	height: 92rpx;
	box-sizing: border-box;
	margin-left: 30rpx;
	padding-right: 30rpx;
	border-bottom: 1rpx solid #e5e5e5;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.my-list-img {
	height: 36rpx;
	width: 36rpx;
	margin-right: 26rpx;
}
.my-list-name {
	font-size: 28rpx;
	color: #474747;
	flex-grow: 1;
}
.my-list-right {
	height: 36rpx;
	width: 36rpx;
}